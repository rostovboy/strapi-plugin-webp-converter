{
  "version": "1.0.3",
  "keywords": [
    "strapi",
    "strapi-plugin",
    "webp",
    "converter",
    "image"
  ],
  "type": "commonjs",
  "exports": {
    "./package.json": "./package.json",
    "./strapi-server": {
      "types": "./dist/server/src/index.d.ts",
      "source": "./server/src/index.ts",
      "import": "./dist/server/index.mjs",
      "require": "./dist/server/index.js",
      "default": "./dist/server/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "strapi-plugin build",
    "watch": "strapi-plugin watch",
    "watch:link": "strapi-plugin watch:link",
    "verify": "strapi-plugin verify",
    "test:ts:back": "run -T tsc -p server/tsconfig.json"
  },
  "dependencies": {
    "sharp": "^0.33.5"
  },
  "devDependencies": {
    "@strapi/sdk-plugin": "^5.2.7",
    "@strapi/strapi": "^5.1.1",
    "@strapi/typescript-utils": "^5.1.1",
    "prettier": "^3.3.3",
    "typescript": "^5.6.3"
  },
  "peerDependencies": {
    "@strapi/sdk-plugin": "^5.2.7",
    "@strapi/strapi": "^5.1.1"
  },
  "strapi": {
    "kind": "plugin",
    "name": "webp-converter",
    "displayName": "Simple WebP Converter",
    "description": "A Strapi plugin that converts images to WebP format."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dnlnwk/strapi-plugin-webp-converter.git"
  },
  "homepage": "https://dnlnwk.de/",
  "name": "strapi-plugin-webp-converter",
  "description": "A Strapi plugin that converts images to WebP format.",
  "license": "MIT",
  "author": "Daniel Nowak <daniel@dnlnwk.de>",
  "maintainers": [
    {
      "name": "Daniel Nowak",
      "email": "daniel@dnlnwk.de",
      "url": "https://dnlnwk.de/"
    }
  ]
}